{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "env": "browser",
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      //"jsx": true,
      "modules": true
    }
  },
  "rules": {
    //"no-undef": 0,
    "no-console": 0
  }
}
