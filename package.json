{
  "name": "tao-webpack",
  "version": "1.0.1",
  "description": "Webpack for TAO ",
  "main": "src/index.js",
  "scripts": {
    "start": "BROWSERSLIST_CONFIG=./.browserslist NODE_ENV=hot node webpack.server.js",
    "watch": "BROWSERSLIST_CONFIG=./.browserslist NODE_ENV=dev webpack --progress --watch",
    "build-prod": "BROWSERSLIST_CONFIG=./.browserslist NODE_ENV=prod webpack",
    "build-dev": "BROWSERSLIST_CONFIG=./.browserslist NODE_ENV=dev webpack",
    "test-comparison": "casperjs test test/comparison.js --user=`whoami` || true",
    "pkg": "npm install && bower install",
    "cached-pkg": "npm-cache install bower npm",
    "install-tests": "sudo npm i -g fs phantomjs-prebuilt phantomcss resemblejs casperjs",
    "install-main": "sudo npm i -g bower webpack babel-cli",
    "lock": "npm shrinkwrap --dev && bower i --reset-shrinkwrap"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.s.intranet:core/frontend-blank.git"
  },
  "author": "web@techart.ru",
  "license": "ISC",
  "dependencies": {
    "bourbon": "^4.2.7",
    "normalize-scss": "^5.0.2",
    "jquery": "^3.1.0"
  },
  "devDependencies": {
    "@webtechart/doiuse": "^2.5.1",
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "6.2.10",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "bower": "^1.7.7",
    "bower-shrinkwrap-resolver": "^0.4.1",
    "bower-webpack-plugin": "^0.1.9",
    "clean-webpack-plugin": "^0.1.8",
    "component-css": "0.0.8",
    "component-css-loader": "^0.1.0",
    "component-resolver-webpack": "^0.4.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-loader": "^1.3.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "image-webpack-loader": "^2.0.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "node-sass": "3.13.1",
    "path": "^0.12.7",
    "postcss-loader": "^0.13.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "stylelint-config-standard": "^13.0.0",
    "stylelint-webpack-plugin": "^0.5.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.x",
    "webpack-dev-server": "^1.14.1",
    "webpack-spritesmith": "^0.2.0",
    "webpack-svgstore-plugin": "^3.0.3"
  }
}
