{
  "name": "tao-webpack",
  "version": "2.2.0",
  "description": "Webpack for TAO",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=hot node webpack.server.js",
    "watch": "NODE_ENV=dev webpack --progress --watch",
    "build-prod": "NODE_ENV=prod webpack",
    "build-dev": "NODE_ENV=dev webpack",
