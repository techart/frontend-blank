    "autoprefixer": "^6.3.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "7.2.x",
    "babel-loader": "6.2.10",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "bower": "^1.7.7",
